    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.1.2",
    "typechain": "^4.0.0",
    "typescript": "^4.0.3",
    "typesync": "^0.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  }
