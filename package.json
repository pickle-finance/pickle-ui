{
  "name": "pickle-app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "BigBrainBriner <bigbrainbriner@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn typechain && next",
    "build": "yarn typechain && next build",
    "start": "yarn typechain && next start",
    "format": "prettier --write \"./**/*.{json,js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "postinstall": "yarn typechain & typesync",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc",
    "typechain": "typechain --target ethers-v5 --outDir ./containers/Contracts './containers/ABIs/*.json'"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "dependencies": {
    "@bit/lekanmedia.shared-ui.date-picker": "^0.0.4",
    "@ethersproject/abstract-provider": "^5.0.4",
    "@geist-ui/react": "^2.0.2",
    "@geist-ui/react-icons": "^1.0.0",
    "@primer/octicons-react": "^11.0.0",
    "@studydefi/money-legos": "^2.3.7",
    "ethers": "^5.0.14",
    "ethers-multicall": "^0.1.3",
    "moment": "^2.29.1",
    "next": "^9.5.3",
    "react": "^16.13.1",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-jazzicon": "^0.1.3",
    "react-number-format": "^4.4.4",
    "react-query": "^3.12.0",
    "rxjs": "^6.6.3",
    "sass": "^1.26.11",
    "styled-components": "^5.2.0",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^1.0.0",
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.11.1",
    "@types/prettier": "2.1.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^17.0.0",
    "@types/react-is": "^17.0.0",
    "@types/sass": "^1.16.0",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.1.2",
    "typechain": "^2.0.0",
    "typescript": "^4.0.3",
    "typesync": "^0.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  }
}
