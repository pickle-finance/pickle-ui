    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "moment": "^2.29.1",
    "next": "^9.5.3",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-apexcharts": "^1.3.7",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.13.1",
    "react-gauge-chart": "^0.3.0",
    "react-is": "^16.13.1",
    "react-jazzicon": "^0.1.3",
    "react-loader-spinner": "^4.0.0",
    "react-number-format": "^4.4.4",
    "react-query": "^3.12.0",
